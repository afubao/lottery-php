{
    "name": "afubao/lottery",
    "description": "A lottery module package for ThinkPHP 8",
    "type": "library",
    "keywords": [
        "lottery",
        "thinkphp",
        "prize",
        "draw"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Leo",
            "email": ""
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "topthink/framework": "^8.0",
        "topthink/think-orm": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Leo\\Lottery\\": "src/"
        }
    },
    "extra": {
        "think": {
            "config": {
                "lottery": "config/lottery.php"
            },
            "services": [
                "Leo\\Lottery\\LotteryServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
